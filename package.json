{
  "name": "CleanAnimals",
  "version": "1.0.9",
  "private": true,
  "scripts": {
    "cp:local:gradle": "cp -f local-gradle ./android/app/build.gradle && cp -f local-gradle.properties ./android/gradle.properties",
    "cp:release:gradle": "cp -f local-gradle.release ./android/app/build.gradle && cp -f local-gradle.properties.release ./android/gradle.properties",
    "android:build:local:release": "npm run cp:local:gradle && cd android && ./gradlew assembleRelease && npm run cp:release:gradle",
    "android:release": "react-native run-android --variant=release",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --watchAll  --detectOpenHandles",
    "ci:test": "jest",
    "lint": "eslint --fix  --ignore-pattern *.test.* .",
    "tsc:watch": "npx tsc --watch",
    "pre:git-action": "npm run lint && npm run ci:test",
    "e2e:build:ios:d": "detox build --configuration ios.emu.debug",
    "e2e:test:ios:d": "detox test --configuration ios.emu.debug --watch",
    "e2e:build:android:d": "detox build --configuration android.emu.debug",
    "e2e:test:android:d": "detox test --configuration android.emu.debug --watch",
    "bump-patch": "npm version patch && bundle exec fastlane bump && npm run commit-version",
    "bump-minor": "npm version minor && bundle exec fastlane bump && npm run commit-version",
    "bump-major": "npm version major && bundle exec fastlane bump && npm run commit-version",
    "commit-version": "git add . && git ci 'new file versions'",
    "codepush:staging": "appcenter codepush release-react -a danielkiesshau/CleanAnimals -c ./bundles/ -d Staging"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre:git-action",
      "pre-push": "npm run pre:git-action"
    }
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/push-notification-ios": "^1.8.0",
    "@react-navigation/bottom-tabs": "^5.10.4",
    "@react-navigation/native": "^5.8.4",
    "@react-navigation/stack": "^5.12.6",
    "appcenter": "4.0.2",
    "appcenter-analytics": "4.0.2",
    "appcenter-crashes": "4.0.2",
    "axios": "^0.21.0",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-content-loader": "^5.1.4",
    "react-native": "0.63.3",
    "react-native-code-push": "^7.0.0",
    "react-native-dotenv": "^2.4.3",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-push-notification": "^6.1.3",
    "react-native-reanimated": "^1.13.1",
    "react-native-safe-area-context": "^3.1.8",
    "react-native-screens": "^2.12.0",
    "react-native-svg": "^12.1.0",
    "react-native-vector-icons": "^7.1.0",
    "reactotron-react-native": "^5.0.0",
    "styled-components": "^5.2.1",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/axios": "^0.14.0",
    "@types/detox": "^16.4.2",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.15",
    "@types/react-native": "^0.63.32",
    "@types/react-native-push-notification": "^5.0.5",
    "@types/react-native-vector-icons": "^6.4.6",
    "@types/styled-components": "^5.1.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.2",
    "babel-plugin-root-import": "^6.6.0",
    "detox": "^17.13.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-native": "^3.10.0",
    "husky": "^4.3.6",
    "jest": "^26.6.2",
    "jest-circus": "^26.6.3",
    "jest-enzyme": "^7.1.2",
    "metro-react-native-babel-preset": "^0.63.0",
    "react-dom": "^17.0.1"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "testPathIgnorePatterns": [
      ".+e2e.+"
    ],
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ]
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
